<h1>Ontario Fishing Records</h1>

<table>
  <tr>
    <th>Species</th>
    <th>Weight (lbs.)</th>
  </tr>

  <% @records.each do |record| %>
    <tr>
      <td><%= record.species %></td>
      <td><%= record.weight %></td>
      <td><%= link_to 'Show', record_path(record) %></td>
    </tr>
  <% end %>
</table>
